                var regularId = '#qty' + formatId + 'Regular' + @(increment);

                if (parseInt($(expediteId).val()) > 0 && !$(regularId).val()) {
                        documentObject@(increment).push({ Name: $(this).val(), Quantity: parseInt($(expediteId).val()) , Transaction: "Expedite"});
                    }
                    else if (parseInt($(regularId).val()) > 0 && !$(expediteId).val()) {
                        documentObject@(increment).push({ Name: $(this).val(), Quantity: parseInt($(regularId).val()) , Transaction: "Regular"});
                    }
                    else if (parseInt($(expediteId).val()) > 0 && parseInt($(regularId).val()) > 0) {
                        documentObject@(increment).push({ Name: $(this).val(), Quantity: parseInt($(expediteId).val()), Transaction: "Expedite" });
                        documentObject@(increment).push({ Name: $(this).val(), Quantity: parseInt($(regularId).val()), Transaction: "Regular" });
                    }
                    else { return; }
