                            documentObject.push({ Name: $(this).val(), Quantity: parseInt($(expediteId).val()) , Transaction: "Expedite"});
                        }
                        else if (parseInt($(regularId).val()) > 0 && !$(expediteId).val()) {
                            documentObject.push({ Name: $(this).val(), Quantity: parseInt($(regularId).val()) , Transaction: "Regular"});
                        }
                        else if (parseInt($(expediteId).val()) > 0 && parseInt($(regularId).val()) > 0) {
                            documentObject.push({ Name: $(this).val(), Quantity: parseInt($(expediteId).val()), Transaction: "Expedite" });
                            documentObject.push({ Name: $(this).val(), Quantity: parseInt($(regularId).val()), Transaction: "Regular" });
                        }
                        else { return; }
                });

                $("#documentsTable tbody > tr").empty();
                $("#documentsTable").append('<tr><th>Document</th><th>Quantity</th><th>Transaction</th></tr>');
                //var tbodyContent = '';
